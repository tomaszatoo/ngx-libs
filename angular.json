{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@tomaszatoo/ngx-wp-api": {
      "projectType": "library",
      "root": "projects/tomaszatoo/ngx-wp-api",
      "sourceRoot": "projects/tomaszatoo/ngx-wp-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/tomaszatoo/ngx-wp-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/tomaszatoo/ngx-wp-api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/tomaszatoo/ngx-wp-api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/tomaszatoo/ngx-wp-api/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "@tomaszatoo/examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/tomaszatoo/examples",
      "sourceRoot": "projects/tomaszatoo/examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/tomaszatoo/examples",
            "index": "projects/tomaszatoo/examples/src/index.html",
            "browser": "projects/tomaszatoo/examples/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/tomaszatoo/examples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/tomaszatoo/examples/public"
              }
            ],
            "styles": [
              "projects/tomaszatoo/examples/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "@tomaszatoo/examples:build:production"
            },
            "development": {
              "buildTarget": "@tomaszatoo/examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/tomaszatoo/examples/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/tomaszatoo/examples/public"
              }
            ],
            "styles": [
              "projects/tomaszatoo/examples/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "@tomaszatoo/ngx-timer": {
      "projectType": "library",
      "root": "projects/tomaszatoo/ngx-timer",
      "sourceRoot": "projects/tomaszatoo/ngx-timer/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/tomaszatoo/ngx-timer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/tomaszatoo/ngx-timer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/tomaszatoo/ngx-timer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/tomaszatoo/ngx-timer/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "@tomaszatoo/graph-viewer": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/tomaszatoo/graph-viewer",
      "sourceRoot": "projects/tomaszatoo/graph-viewer/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/tomaszatoo/graph-viewer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/tomaszatoo/graph-viewer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/tomaszatoo/graph-viewer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/tomaszatoo/graph-viewer/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
